ion: relative;\r\n  color: #f0f0f0;\r\n  cursor: pointer;\r\n  z-index: 1;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  white-space: nowrap;\r\n  user-select: none;\r\n  -webkit-user-select: none;\r\n  touch-action: manipulation;\r\n}\r\n\r\n#play-pause-button::after,\r\n#play-pause-button::before {\r\n  content: '';\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  z-index: -99999;\r\n  transition: all .4s;\r\n}\r\n\r\n#play-pause-button::before {\r\n  transform: translate(0%, 0%);\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(255, 255, 255, 0.1);\r\n  border-radius: 10px;\r\n}\r\n\r\n#play-pause-button::after {\r\n  transform: translate(10px, 10px);\r\n  width: 35px;\r\n  height: 35px;\r\n  background: purple;\r\n  backdrop-filter: blur(5px);\r\n  -webkit-backdrop-filter: blur(5px);\r\n  border-radius: 50px;\r\n}\r\n\r\n#play-pause-button:hover::before {\r\n  transform: translate(5%, 20%);\r\n  width: 110%;\r\n  height: 110%;\r\n}\r\n\r\n#play-pause-button:hover::after {\r\n  border-radius: 10px;\r\n  transform: translate(0, 0);\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n#play-pause-button:active::after {\r\n  transition: 0s;\r\n  transform: translate(0, 5%);\r\n}\r\n#waveform {\r\n  width: 76%;\r\n  overflow: hidden;\r\n}\r\n#time,\r\n#duration {\r\n  position: absolute;\r\n  z-index: 3;\r\n  top: 52.7%;\r\n  margin-top: -1px;\r\n  transform: translateY(-50%);\r\n  font-size: 11px;\r\n  background: rgba(0, 0, 0, 0.75);\r\n  padding: 2px;\r\n  color: #ddd;\r\n}\r\n#time {\r\n  left: 10.5%;\r\n}\r\n#duration {\r\n  right: 10.5%;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"9eb5d6b3d4b81a9b62dfbe74c058a925","cacheData":{"env":{}}}